.newAccount:active {
  border: 1px dashed red;
}

.newAccount > .desc {
  width: 70%;
  margin: 25% auto;
  color: #838383;
  line-height: 60rpx;
  text-align: center;
}

.newAccount > image {
  height: 90rpx;
  width: 90rpx;
}

.newAccount > .title {
  color: #D3D3D3;
  font-size: 40rpx;
  margin-top: 40rpx;
}

.list {
  z-index: 1;
}

.list, .list + image {
  position: absolute;
  left: 25rpx;
  top: 25rpx;
  height: 60rpx;
  width: 60rpx;
}

swiper {
  height: 100%;
  width: 100%;
}

.updateImg {
  position: absolute;
  height: 40rpx;
  width: 40rpx;
  right: 20rpx;
  bottom: 20rpx;
}
