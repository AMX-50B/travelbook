  height: 100%;
  background-color: #3A3A3A;
}

.newAccount {
  margin: 20% auto 10%;
  height: 80%;
  width: 90%;
  border-radius: 20rpx;
}

.newAccount {
  display: flex;
  flex-direction: column;
  align-items: center;
  border: 1px dashed #acabab;
}

